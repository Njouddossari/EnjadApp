<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lenovo.enjad.ProfileActivity"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintVertical_bias="0.523">

        <EditText
            android:id="@+id/Phoneet"
            style="@style/Widget.AppCompat.AutoCompleteTextView"
            android:layout_width="149dp"
            android:layout_height="45dp"
            android:layout_weight="1"
            android:ems="10"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="28dp"
            tools:layout_editor_absoluteY="207dp" />

        <TextView
            android:id="@+id/Phonevt"
            android:layout_width="123dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/phone"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textColor="@color/colorPrimaryRed"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="214dp"
            tools:layout_editor_absoluteY="210dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="0.36"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="1dp"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        android:id="@+id/linearLayout4">

        <EditText
            android:id="@+id/Emailet"
            style="@style/Widget.AppCompat.AutoCompleteTextView"
            android:layout_width="149dp"
            android:layout_height="45dp"
            android:ems="10"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="31dp"
            tools:layout_editor_absoluteY="194dp" />

        <TextView
            android:id="@+id/emailtv"
            android:layout_width="123dp"
            android:layout_height="34dp"
            android:layout_weight="1"
            android:text="@string/email"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@color/colorPrimaryRed"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="214dp"
            tools:layout_editor_absoluteY="129dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        android:id="@+id/linearLayout2"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="43dp"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4">

        <EditText
            android:id="@+id/Passet"
            style="@style/Widget.AppCompat.AutoCompleteTextView"
            android:layout_width="149dp"
            android:layout_height="45dp"
            android:ems="10"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="28dp"
            tools:layout_editor_absoluteY="207dp" />

        <TextView
            android:id="@+id/passwrdtv"
            android:layout_width="123dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/Password"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textColor="@color/colorPrimaryRed"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="214dp"
            tools:layout_editor_absoluteY="210dp" />
    </LinearLayout>

    <include
        layout="@layout/shared_design"
        android:layout_height="0dp"
        android:layout_width="0dp"
        app:layout_constraintRight_toLeftOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:layout_constraintLeft_creator="1"
        android:id="@+id/linearLayout3">

        <EditText
            android:id="@+id/Usrnameet"
            style="@style/Widget.AppCompat.AutoCompleteTextView"
            android:layout_width="149dp"
            android:layout_height="45dp"
            android:ems="10"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="31dp"
            tools:layout_editor_absoluteY="69dp" />

        <TextView
            android:id="@+id/usernametv"
            android:layout_width="123dp"
            android:layout_height="34dp"
            android:layout_weight="1"
            android:text="اسم المستخدم:"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@color/colorPrimaryRed"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="184dp"
            tools:layout_editor_absoluteY="87dp" />
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/profile"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textSize="36sp"
        android:textColor="@color/colorPrimaryBlack"
        android:id="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <Switch
        android:id="@+id/providehelpSW"
        android:layout_width="176dp"
        android:layout_height="0dp"
        android:layout_marginLeft="16dp"
        android:checked="false"
        android:gravity="right|center"
        android:padding="75dp"
        android:splitTrack="false"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/savechngeBU"
        android:layout_marginStart="16dp"
        android:layout_marginTop="44dp"
        android:layout_marginBottom="33dp"
        app:layout_constraintTop_toTopOf="@+id/linearLayout2" />

    <TextView
        android:id="@+id/accesset"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:text="@string/accessTocontact"
        android:textAllCaps="true"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="7dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="116dp"
        android:layout_marginBottom="119dp"
        app:layout_constraintTop_toTopOf="@+id/providehelpSW"
        android:layout_marginRight="7dp" />

    <TextView
        android:id="@+id/provideet"
        android:layout_width="177dp"
        android:layout_height="32dp"
        android:text="@string/providehelp"
        android:textAllCaps="true"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/savechngeBU"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="158dp"
        android:layout_marginBottom="158dp"
        app:layout_constraintTop_toTopOf="@+id/linearLayout" />

    <Switch
        android:id="@+id/accesstocontctSW"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintRight_toLeftOf="@+id/provideet"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/providehelpSW"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="22dp"
        android:layout_marginRight="23dp" />

    <Button
        android:id="@+id/savechngeBU"
        android:layout_width="176dp"
        android:layout_height="62dp"
        android:text="@string/savechenges"
        android:theme="@style/ButtonText"
        android:background="@drawable/button_desing"
        android:textColor="@color/colorPrimaryRed"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="6dp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="28dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.334"
        android:layout_marginStart="28dp"
        android:id="@+id/linearLayout">

        <EditText
            android:id="@+id/Helathet"
            style="@style/Widget.AppCompat.AutoCompleteTextView"
            android:layout_width="144dp"
            android:layout_height="45dp"
            android:ems="10"
            android:inputType="textPersonName"
            tools:layout_editor_absoluteX="31dp"
            tools:layout_editor_absoluteY="114dp" />

        <TextView
            android:id="@+id/healthtv"
            android:layout_width="wrap_content"
            android:layout_height="34dp"
            android:layout_marginEnd="50dp"
            android:layout_marginRight="50dp"
            android:text="@string/my_healthcondition"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@color/colorPrimaryRed"
            android:textSize="18sp"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteY="172dp" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
