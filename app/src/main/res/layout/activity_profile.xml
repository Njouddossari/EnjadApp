<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lenovo.enjad.ProfileActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="25dp">

    <include
        android:id="@+id/include5"
        layout="@layout/shared_design"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"
        tools:ignore="MissingConstraints" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="76dp"
        tools:layout_editor_absoluteY="0dp"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/PersonalProfile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dp"
            android:text="@string/profile"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@color/colorPrimaryBlack"
            android:textSize="36sp"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1"
            tools:layout_editor_absoluteX="94dp"
            tools:layout_editor_absoluteY="16dp" />

    </RelativeLayout>

    <EditText
        android:id="@+id/Usrnameet"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="184dp"
        android:layout_height="45dp"
        android:layout_above="@+id/imageView11"
        android:layout_centerHorizontal="true"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/colorPrimaryBlack"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="98dp" />

    <EditText
        android:id="@+id/Helathet"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="184dp"
        android:layout_height="45dp"
        android:layout_alignLeft="@+id/Phoneet"
        android:layout_alignStart="@+id/Phoneet"
        android:layout_below="@+id/Emailet"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/colorPrimaryBlack"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="206dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/Passet"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="184dp"
        android:layout_height="45dp"
        android:layout_alignLeft="@+id/savechngeBU"
        android:layout_alignStart="@+id/savechngeBU"
        android:layout_alignTop="@+id/imageView12"
        android:layout_weight="1"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/colorPrimaryBlack"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="316dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/accesset"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/provideet"
        android:text="@string/accessTocontact"
        android:textAllCaps="true"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteX="196dp"
        tools:layout_editor_absoluteY="432dp"
        tools:ignore="MissingConstraints" />

    <Switch
        android:id="@+id/accesstocontctSW"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Phoneet"
        android:layout_alignStart="@+id/Phoneet"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="432dp"
        tools:ignore="MissingConstraints" />

    <Switch
        android:id="@+id/providehelpSW"
        android:layout_width="68dp"
        android:layout_height="@android:dimen/notification_large_icon_width"
        android:layout_alignLeft="@+id/Helathet"
        android:layout_alignStart="@+id/Helathet"
        android:layout_alignTop="@+id/imageView11"
        android:checked="false"
        android:gravity="right|center"
        android:padding="75dp"
        android:splitTrack="false"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="368dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="@android:dimen/app_icon_size"
        android:layout_height="@android:dimen/app_icon_size"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/imageView12"
        android:layout_toRightOf="@+id/imageView12"
        app:srcCompat="@drawable/email"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="143dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="@android:dimen/app_icon_size"
        android:layout_height="@android:dimen/app_icon_size"
        android:layout_alignLeft="@+id/include5"
        android:layout_alignStart="@+id/include5"
        android:layout_below="@+id/providehelpSW"
        app:srcCompat="@drawable/username"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="88dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="@android:dimen/app_icon_size"
        android:layout_height="@android:dimen/app_icon_size"
        android:layout_above="@+id/imageView10"
        android:layout_alignLeft="@+id/imageView10"
        android:layout_alignStart="@+id/imageView10"
        android:layout_weight="1"
        app:srcCompat="@drawable/healthinfo"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="203dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/savechngeBU"
        android:layout_width="176dp"
        android:layout_height="62dp"
        android:layout_alignTop="@+id/include5"
        android:layout_toLeftOf="@+id/include5"
        android:layout_toStartOf="@+id/include5"
        android:background="@drawable/button_desing"
        android:text="@string/savechenges"
        android:theme="@style/ButtonText"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="480dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/Emailet"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="184dp"
        android:layout_height="45dp"
        android:layout_alignEnd="@+id/providehelpSW"
        android:layout_alignRight="@+id/providehelpSW"
        android:layout_weight="1"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/colorPrimaryBlack"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="151dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/Phoneet"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="184dp"
        android:layout_height="45dp"
        android:layout_above="@+id/imageView11"
        android:layout_toLeftOf="@+id/Usrnameet"
        android:layout_toStartOf="@+id/Usrnameet"
        android:layout_weight="1"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/colorPrimaryBlack"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="261dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/provideet"
        android:layout_width="164dp"
        android:layout_height="32dp"
        android:layout_alignBottom="@+id/imageView10"
        android:text="@string/providehelp"
        android:textAllCaps="true"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_editor_absoluteX="196dp"
        tools:layout_editor_absoluteY="384dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="@android:dimen/app_icon_size"
        android:layout_height="@android:dimen/app_icon_size"
        app:srcCompat="@drawable/phone"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="259dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="@android:dimen/app_icon_size"
        android:layout_height="@android:dimen/app_icon_size"
        app:srcCompat="@drawable/pw"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="313dp"
        tools:ignore="MissingConstraints" />

</android.support.constraint.ConstraintLayout>
