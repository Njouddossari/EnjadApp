<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lenovo.enjad.ProfileActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="25dp">

    <include
        android:id="@+id/include5"
        layout="@layout/shared_design"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="55dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="52dp">

        <TextView
            android:id="@+id/PersonalProfile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:text="@string/profile"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@color/colorPrimaryBlack"
            android:textSize="36sp"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1"
            tools:layout_editor_absoluteX="80dp"
            tools:layout_editor_absoluteY="49dp" />
    </RelativeLayout>

    <EditText
        android:id="@+id/Helathet"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="184dp"
        android:layout_height="45dp"
        android:layout_alignLeft="@+id/Phoneet"
        android:layout_alignStart="@+id/Phoneet"
        android:layout_below="@+id/Emailet"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/colorPrimaryBlack"
        tools:ignore="MissingConstraints,NotSibling"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="206dp" />

    <EditText
        android:id="@+id/Passet"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="184dp"
        android:layout_height="45dp"
        android:layout_alignLeft="@+id/savechngeBU"
        android:layout_alignStart="@+id/savechngeBU"
        android:layout_alignTop="@+id/imageView12"
        android:layout_weight="1"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/colorPrimaryBlack"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="316dp" />

    <TextView
        android:id="@+id/accesset"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/provideet"
        android:text="@string/accessTocontact"
        android:textAllCaps="true"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="196dp"
        tools:layout_editor_absoluteY="432dp" />

    <Switch
        android:id="@+id/providehelpSW"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Phoneet"
        android:layout_alignStart="@+id/Phoneet"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="384dp" />

    <Switch
        android:id="@+id/accesstocontctSW"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Phoneet"
        android:layout_alignStart="@+id/Phoneet"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="432dp" />

    <RelativeLayout
        android:layout_width="254dp"
        android:layout_height="51dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="155dp">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="@android:dimen/app_icon_size"
            android:layout_height="@android:dimen/app_icon_size"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            app:srcCompat="@drawable/email"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="280dp"
            tools:layout_editor_absoluteY="143dp" />

        <EditText
            android:id="@+id/Emailet"
            style="@style/Widget.AppCompat.AutoCompleteTextView"
            android:layout_width="184dp"
            android:layout_height="45dp"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="19dp"
            android:layout_marginRight="19dp"
            android:layout_toLeftOf="@+id/imageView11"
            android:layout_toStartOf="@+id/imageView11"
            android:layout_weight="1"
            android:ems="10"
            android:inputType="textPersonName"
            android:textColor="@color/colorPrimaryBlack"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="20dp"
            tools:layout_editor_absoluteY="0dp" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_above="@+id/imageView10"
        android:layout_alignLeft="@+id/imageView10"
        android:layout_alignStart="@+id/imageView10"
        android:layout_weight="1"
        app:srcCompat="@drawable/healthinfo"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="203dp" />

    <Button
        android:id="@+id/savechngeBU"
        android:layout_width="176dp"
        android:layout_height="62dp"
        android:layout_alignTop="@+id/include5"
        android:layout_toLeftOf="@+id/include5"
        android:layout_toStartOf="@+id/include5"
        android:background="@drawable/button_desing"
        android:text="@string/savechenges"
        android:theme="@style/ButtonText"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="480dp" />


    <EditText
        android:id="@+id/Phoneet"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="184dp"
        android:layout_height="45dp"
        android:layout_above="@+id/imageView11"
        android:layout_toLeftOf="@+id/Usrnameet"
        android:layout_toStartOf="@+id/Usrnameet"
        android:layout_weight="1"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/colorPrimaryBlack"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="261dp" />

    <TextView
        android:id="@+id/provideet"
        android:layout_width="164dp"
        android:layout_height="32dp"
        android:layout_alignBottom="@+id/imageView10"
        android:text="@string/providehelp"
        android:textAllCaps="true"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints,NotSibling"
        tools:layout_editor_absoluteX="196dp"
        tools:layout_editor_absoluteY="384dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/phone"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="259dp" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/pw"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="313dp" />

    <EditText
        android:id="@+id/contact3et"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_weight="1"
        android:ems="10"
        android:hint="@string/phone_et"
        android:inputType="phone"
        android:textColor="@color/colorPrimaryBlack"
        android:visibility="invisible"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="196dp"
        tools:layout_editor_absoluteY="464dp" />

    <EditText
        android:id="@+id/contact2et"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="@string/phone_et"
        android:inputType="phone"
        android:textColor="@color/colorPrimaryBlack"
        android:visibility="invisible"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="186dp"
        tools:layout_editor_absoluteY="497dp" />

    <EditText
        android:id="@+id/contact1et"
        style="@style/Widget.AppCompat.AutoCompleteTextView"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_marginBottom="16dp"
        android:ems="10"
        android:hint="@string/phone_et"
        android:inputType="textPersonName"
        android:textColor="@color/colorPrimaryBlack"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="186dp" />

    <RelativeLayout
        android:layout_width="253dp"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="107dp">


        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="@android:dimen/app_icon_size"
            android:layout_height="@android:dimen/app_icon_size"
            android:layout_alignBottom="@+id/Usrnameet"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            app:srcCompat="@drawable/username"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="280dp"
            tools:layout_editor_absoluteY="143dp" />

        <EditText
            android:id="@+id/Usrnameet"
            style="@style/Widget.AppCompat.AutoCompleteTextView"
            android:layout_width="184dp"
            android:layout_height="45dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:ems="10"
            android:inputType="textPersonName"
            android:textColor="@color/colorPrimaryBlack"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="107dp" />
    </RelativeLayout>


</android.support.constraint.ConstraintLayout>