<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lenovo.enjad.SignupActivity"
    tools:ignore="GradleDependency"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="25dp">

    <include
        android:id="@+id/include2"
        layout="@layout/shared_design"
        tools:ignore="MissingConstraints"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_height="0dp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_width="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/passwrdtv"
        android:layout_width="0dp"
        android:layout_height="29dp"
        android:text="@string/Password"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="22sp"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/reenterpasstv"
        android:layout_marginEnd="4dp"
        app:layout_constraintRight_toRightOf="@+id/passwrdet"
        android:layout_marginTop="73dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="70dp"
        app:layout_constraintTop_toTopOf="@+id/phonetv"
        app:layout_constraintLeft_toRightOf="@+id/signbu"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp" />

    <TextView
        android:id="@+id/emailtv"
        android:layout_width="137dp"
        android:layout_height="29dp"
        android:text="@string/email"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="22sp"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:ignore="MissingConstraints"
        app:layout_constraintBottom_toTopOf="@+id/phonetv"
        android:layout_marginStart="83dp"
        android:layout_marginEnd="83dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="114dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="114dp"
        app:layout_constraintLeft_toLeftOf="@+id/reenterpasset"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="83dp"
        android:layout_marginRight="83dp" />

    <TextView
        android:id="@+id/usrnametv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/username"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="22sp"
        tools:layout_constraintRight_creator="1"
        tools:ignore="MissingConstraints"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/usrnameet"
        android:layout_marginEnd="2dp"
        app:layout_constraintRight_toRightOf="@+id/usrnameet"
        android:layout_marginBottom="4dp"
        android:layout_marginRight="2dp" />

    <EditText
        android:id="@+id/passwrdet"
        android:layout_width="225dp"
        android:layout_height="35dp"
        android:background="@drawable/edittext_design"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:ignore="MissingConstraints"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/reenterpasset"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="72dp"
        android:layout_marginBottom="71dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/phoneet" />

    <EditText
        android:id="@+id/emailet"
        android:layout_width="221dp"
        android:layout_height="33dp"
        android:background="@drawable/edittext_design"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:ignore="MissingConstraints"
        app:layout_constraintBottom_toBottomOf="@+id/healthtv"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="29dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="31dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/emailtv" />

    <EditText
        android:id="@+id/usrnameet"
        android:layout_width="221dp"
        android:layout_height="37dp"
        android:background="@drawable/edittext_design"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:ignore="MissingConstraints"
        app:layout_constraintBottom_toBottomOf="@+id/emailet"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="69dp"
        android:layout_marginBottom="70dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/signbu"
        android:layout_width="117dp"
        android:layout_height="wrap_content"
        android:background="@drawable/button_desing"
        android:onClick="signuponclick"
        android:text="@string/signup"
        android:theme="@style/ButtonText"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:ignore="MissingConstraints"
        android:layout_marginStart="80dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="80dp" />

    <EditText
        android:id="@+id/reenterpasset"
        android:layout_width="223dp"
        android:layout_height="32dp"
        android:background="@drawable/edittext_design"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:ignore="MissingConstraints"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="103dp"
        android:layout_marginBottom="102dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/passwrdtv" />

    <TextView
        android:id="@+id/healthtv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/my_healthcondition"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="22sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:ignore="MissingConstraints"
        android:layout_marginEnd="3dp"
        app:layout_constraintRight_toRightOf="@+id/healthet"
        android:layout_marginTop="38dp"
        app:layout_constraintTop_toBottomOf="@+id/emailtv"
        android:layout_marginRight="3dp" />

    <EditText
        android:id="@+id/healthet"
        android:layout_width="224dp"
        android:layout_height="34dp"
        android:background="@drawable/edittext_design"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:ignore="MissingConstraints"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/phonetv"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toBottomOf="@+id/emailet"
        android:layout_marginBottom="38dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/phonetv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/phone"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="22sp"
        tools:layout_constraintRight_creator="1"
        tools:ignore="MissingConstraints"
        tools:layout_constraintTop_creator="1"
        android:layout_marginEnd="2dp"
        app:layout_constraintRight_toRightOf="@+id/phoneet"
        android:layout_marginTop="257dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="2dp" />

    <EditText
        android:id="@+id/phoneet"
        android:layout_width="221dp"
        android:layout_height="35dp"
        android:background="@drawable/edittext_design"
        android:ems="10"
        android:inputType="phone"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:ignore="MissingConstraints"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/reenterpasset"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="111dp"
        app:layout_constraintTop_toBottomOf="@+id/emailet"
        android:layout_marginBottom="111dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/reenterpasstv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="122dp"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="91dp"
        android:layout_marginTop="120dp"
        android:text="@string/ConfirmPassword"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/colorPrimaryRed"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/signbu"
        app:layout_constraintRight_toRightOf="@+id/reenterpasset"
        app:layout_constraintTop_toBottomOf="@+id/phonetv"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

</android.support.constraint.ConstraintLayout>
