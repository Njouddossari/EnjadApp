<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lenovo.enjad.SignupActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp"
    tools:ignore="GradleDependency">
    <include layout="@layout/shared_design"/>
    <TextView
        android:id="@+id/passwrdtv"
        android:layout_width="130dp"
        android:layout_height="26dp"
        android:text="@string/Password"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="24sp"
        android:layout_marginEnd="1dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/signbu"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="122dp"
        android:layout_marginBottom="122dp"
        app:layout_constraintTop_toTopOf="@+id/healthet"
        android:layout_marginRight="1dp" />

    <TextView
        android:id="@+id/emailtv"
        android:layout_width="232dp"
        android:layout_height="0dp"
        android:text="@string/email"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="24sp"
        android:layout_marginEnd="1dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/emailet"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="52dp"
        android:layout_marginBottom="34dp"
        app:layout_constraintTop_toTopOf="@+id/usrnameet"
        android:layout_marginRight="1dp" />

    <TextView
        android:id="@+id/usrnametv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/username"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="24sp"
        android:layout_marginEnd="1dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="1dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="1dp" />

    <EditText
        android:id="@+id/passwrdet"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/reenterpasstv"
        app:layout_constraintRight_toRightOf="@+id/phoneet"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/phoneet" />

    <EditText
        android:id="@+id/emailet"
        android:layout_width="221dp"
        android:layout_height="35dp"
        android:layout_marginEnd="54dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/phonetv"
        app:layout_constraintRight_toRightOf="@+id/healthtv"
        android:layout_marginTop="108dp"
        android:layout_marginBottom="109dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="54dp" />

    <EditText
        android:id="@+id/usrnameet"
        android:layout_width="0dp"
        android:layout_height="37dp"
        android:ems="10"
        android:hint="username"
        android:inputType="textPersonName"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/emailet"
        app:layout_constraintRight_toRightOf="@+id/healthet"
        android:layout_marginTop="28dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="42dp"
        app:layout_constraintLeft_toLeftOf="@+id/emailet"
        app:layout_constraintTop_toTopOf="@+id/usrnametv" />

    <Button
        android:id="@+id/signbu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="signuponclick"
        android:text="@string/signup"
        android:textSize="24sp"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="31dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/reenterpasset"
        android:layout_marginLeft="31dp" />

    <TextView
        android:id="@+id/reenterpasstv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="1dp"
        android:text="@string/ConfirmPassword"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="24sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="123dp"
        android:layout_marginRight="1dp" />

    <EditText
        android:id="@+id/reenterpasset"
        android:layout_width="219dp"
        android:layout_height="37dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/signbu"
        android:layout_marginTop="28dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="22dp"
        app:layout_constraintLeft_toLeftOf="@+id/passwrdet"
        app:layout_constraintTop_toTopOf="@+id/reenterpasstv" />

    <TextView
        android:id="@+id/healthtv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/my_healthcondition"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="24sp"
        android:layout_marginEnd="1dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailet"
        android:layout_marginRight="1dp" />

    <EditText
        android:id="@+id/healthet"
        android:layout_width="0dp"
        android:layout_height="37dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/phoneet"
        app:layout_constraintRight_toRightOf="@+id/emailet"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="38dp"
        app:layout_constraintLeft_toLeftOf="@+id/emailet" />

    <TextView
        android:id="@+id/phonetv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/phone"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="24sp"
        android:layout_marginEnd="1dp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/phoneet"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="1dp" />

    <EditText
        android:id="@+id/phoneet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="phone"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/passwrdet"
        app:layout_constraintRight_toRightOf="@+id/healthet"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="30dp"
        app:layout_constraintLeft_toLeftOf="@+id/healthet" />

</android.support.constraint.ConstraintLayout>
